#include<stdio.h>
#include<math.h>
int main(){
    
int i, N, count, n;

    printf("enter a number\n");
    scanf("%d", &N);
            for (n=2; n<=N; n++){
            i=2;
            count=0;
                while(i < n){
                  if (n%i==0){
                   count++;
}
                    i++;
}
                        if(count==0){
                    printf("%d\n", n);
}
}
}
